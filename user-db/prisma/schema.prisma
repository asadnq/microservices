// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output = "../src/lib/generated"
}

datasource db {
  provider = "postgresql"
  url      = env("USER_PG_URL")
}

model User {
  id        String   @id() @default(uuid())
  email     String
  username  String
  createdAt DateTime @default(now())
}
